{
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "mapRoot": "/",
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "outDir": "../dist/",
    "rootDir": ".",
    "sourceMap": true,
    "target": "es5",
    "inlineSources": true
  },
  "filesGlob": [
    "./**/*.ts",
    "!./node_modules"
  ],
  "atom": {
    "rewriteTsconfig": true
  },
  "files": [
    "./app/environment.ts",
    "./app/index.ts",
    "./app/movie-search.service.ts",
    "./app/schedule.view.ts",
    "./app/screening.view.ts",
    "./app/series-list.view.ts",
    "./app/series.view.ts",
    "./app/shared/index.ts",
    "./electron-main.ts",
    "./main.ts",
    "./system-config.ts",
    "./typings.d.ts"
  ]
}
